<core:FragmentDefinition xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<Toolbar class="customMasterFooter">
		<Button id="lockMBtn" type="default" icon="sap-icon://locked"
			press="onPressMasterLock" class="sapUiSmallMarginBegin"
			tooltip="{i18n>lock}" />
		<ToolbarSpacer></ToolbarSpacer>
		<Button id="copyMBtn" icon="sap-icon://copy"
			press="onPressMasterCopy" tooltip="{i18n>copy}" />
		<ToolbarSpacer></ToolbarSpacer>
		<Button id="addMBtn" icon="sap-icon://add"
			press="onPressMasterAdd" tooltip="{i18n>add}" />
		<ToolbarSpacer></ToolbarSpacer>
		<Button id="deleteMBtn" icon="sap-icon://delete"
			class="sapUiSmallMarginEnd" press="onPressMasterDelete"
			tooltip="{i18n>delete}" />
	</Toolbar>
</core:FragmentDefinition>