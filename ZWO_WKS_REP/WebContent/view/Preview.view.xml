<mvc:View controllerName="zwo.ui.wks_rep.controller.Preview"
	xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:core="sap.ui.core"
	xmlns:l="sap.ui.layout" displayBlock="true" height="100%"
	busy="{PreviewView>/busy}"
	busyIndicatorDelay="{PreviewView>/BusyDelay}">
	<Page id="preview" title="{i18n>previewTitle}"
		enableScrolling="true" onAfterRendering="onMainPageRendered"
		class="projectContent">
		<customHeader>
			<Bar>
				<contentLeft>
					<Button type="default" icon="sap-icon://nav-back"
						press="onNavHome" class="previewNavBtn">
					</Button>
				</contentLeft>
				<contentMiddle>
					<Title level="H2" text="{i18n>previewPageTitle}" />
				</contentMiddle>
			</Bar>
		</customHeader>
		<Panel class="customSubHeaderBackground printHeader"
			width="100%">
			<headerToolbar class="sapUiResponsiveMargin">
				<Toolbar>
					<Title level="H2" text="{i18n>filterProject}"
						class="customPreviewHeaderTitle sapUiTinyMarginBottom" />
				</Toolbar>
			</headerToolbar>
			<content>
				<l:Grid id="projectGrid" defaultSpan="L2 M3 S12"
					class="sapUiNoMargin sapUiNoContentPadding">
					<VBox>
						<Label text="{i18n>foremanSelected}"
							class="customHeaderTextColor" />
						<SearchField id="selectForeman"
							class="mySelectForeman customLabelReport" width="auto"
							placeholder="{i18n>dBSelectPlacehdr}" maxLength="1"
							liveChange="onNoInputLiveChange" search="onSelectForeman" />
					</VBox>
					<VBox>
						<Label text="{i18n>statusToDisplay}"
							class="customHeaderTextColor" />
						<SearchField id="selectStatus" class="mySelectForeman"
							width="auto" placeholder="{i18n>statusTxt}" maxLength="1"
							search="onSelectStatus" />
					</VBox>
					<VBox class="sapUiSmallMarginTop">
						<Button text="{i18n>filterButtonText}"
							press="onPressPreviewFilter"
							class="customFilterButton customFilterPreviewBtn" />
					</VBox>
				</l:Grid>
			</content>
		</Panel>
		<Panel id="synthesisBonus" class="sapUiSmallMargin" width="98%"
			visible="true">
			<content>
				<Toolbar height="50px"
					visible="{= ${laborBonus>/}.length !== 0}">
					<Text text="{i18n>LaborOther}"
						class="sapUiSmallMarginTopBottom customPreviewFont">
					</Text>
					<ToolbarSpacer />
					<VBox height="45px">
						<Button visible="{= !${laborBonus>/Edit}}"
							icon="sap-icon://edit" press="onPressPreviewBonusEdit"
							type="Transparent" class="sapUiSmallMarginBottom printButton" />
						<HBox>
							<Button visible="{= ${laborBonus>/Edit}}"
								icon="sap-icon://save" press="onPressPreviewBonusSave"
								type="Transparent" class="customSaveBtn" />
							<Button visible="{= ${laborBonus>/Edit}}"
								icon="sap-icon://decline" press="onPressPreviewBonusCancel"
								type="Transparent" class="customSaveBtn" />
						</HBox>
					</VBox>
				</Toolbar>
				<ScrollContainer class="" height="" vertical="true">
					<Table items="{laborBonus>/}" height="200px"
						class="sapUiNoMargin sapUiNoContentPadding customTable"
						visible="{= ${laborBonus>/}.length !== 0}">
						<columns class="sapUiNoContentPadding">
							<Column width="5rem">
								<Text text="{i18n>empNo}" />
							</Column>
							<Column width="8rem" hAlign="Left" minScreenWidth="768px"
								demandPopin="true">
								<Text text="{i18n>empName}" />
							</Column>
							<Column width="6rem" hAlign="Left" minScreenWidth="768px"
								demandPopin="true">
								<Text text="{i18n>timePointingCode}" />
							</Column>
							<Column width="5rem" hAlign="Left" minScreenWidth="768px"
								demandPopin="true">
								<Text text="{i18n>timeQty}" />
							</Column>
							<Column width="6rem" hAlign="Left" minScreenWidth="768px"
								demandPopin="true">
								<Text text="{i18n>primeCode}" />
							</Column>
							<Column width="6rem" hAlign="Left" minScreenWidth="768px"
								demandPopin="true">
								<Text text="{i18n>primeQty}" />
							</Column>
							<Column width="6rem" hAlign="Left" minScreenWidth="768px"
								demandPopin="true">
								<Text text="{i18n>wbsElement}" />
							</Column>
							<Column width="5rem" hAlign="Left" minScreenWidth="768px"
								demandPopin="true">
								<Text text="{i18n>activityType}" />
							</Column>
						</columns>
						<items>
							<ColumnListItem
								highlight="{ 
												path: 'laborBonus>IntStatus',
												formatter: '.formatter.setDetStatus'}">
								<cells>
									<Text text="{laborBonus>EmployeeNo}" class="customText"></Text>
									<Text text="{laborBonus>EmployeeName}" class="customText"></Text>
									<VBox justifyContent="Center">
										<Text text="{laborBonus>TypeOfHour}" class="customText"
											visible="{= !${laborBonus>Edit} }" />
										<Input editable="false" type="Text"
											value="{laborBonus>TypeOfHour}" class="customText"
											visible="{= ${laborBonus>Edit} }" width="100%"
											valueHelpRequest="onRequestTypeOfHour" showValueHelp="true" />
									</VBox>
									<VBox justifyContent="Center">
										<Text text="{laborBonus>HoursQty}" class="customText"
											visible="{= !${laborBonus>Edit} }" />
										<Input
											editable="{= ${laborBonus>Edit} &amp;&amp; 
																	${laborBonus>IntStatus} !== '2' &amp;&amp; 
																	${laborBonus>IntStatus} !== '4' &amp;&amp; 
																	${laborBonus>IntStatus} !== '6' &amp;&amp; 
																	${laborBonus>IntStatus} !== '7' &amp;&amp;
																	${laborBonus>Bonus} === '' &amp;&amp; 
																	${laborBonus>BonusQty} === '0.00'}"
											type="Number" value="{laborBonus>HoursQty}"
											class="customText" visible="{= ${laborBonus>Edit} }"
											width="100%" />
									</VBox>
									<VBox justifyContent="Center">
										<Text text="{laborBonus>Bonus}" class="customText"
											visible="{= !${laborBonus>Edit} }" />
										<Input editable="false" type="Text"
											value="{laborBonus>Bonus}" class="customText"
											visible="{= ${laborBonus>Edit} }" width="100%"
											valueHelpRequest="onRequestBonus" showValueHelp="true" />
									</VBox>
									<VBox justifyContent="Center">
										<Text text="{laborBonus>BonusQty}" class="customText"
											visible="{= !${laborBonus>Edit} }" />
										<Input
											editable="{= ${laborBonus>Edit} &amp;&amp; 
																	${laborBonus>IntStatus} !== '2' &amp;&amp; 
																	${laborBonus>IntStatus} !== '4' &amp;&amp; 
																	${laborBonus>IntStatus} !== '6' &amp;&amp; 
																	${laborBonus>IntStatus} !== '7' &amp;&amp;
																	${laborBonus>TypeOfHour} === '' &amp;&amp; 
																	${laborBonus>HoursQty} === '0.00'}"
											type="Number" value="{laborBonus>BonusQty}"
											class="customText" visible="{= ${laborBonus>Edit} }"
											width="100%" />
									</VBox>
									<VBox justifyContent="Center">
										<Text text="{laborBonus>WBSElement}" class="customText"
											visible="{= !${laborBonus>Edit} }" />
										<Input
											editable="{= ${laborBonus>Edit} &amp;&amp; 
																	${laborBonus>IntStatus} !== '2' &amp;&amp; 
																	${laborBonus>IntStatus} !== '4' &amp;&amp; 
																	${laborBonus>IntStatus} !== '6' &amp;&amp; 
																	${laborBonus>IntStatus} !== '7'}"
											type="Text" value="{laborBonus>WBSElement}"
											class="customText" visible="{= ${laborBonus>Edit} }"
											width="100%" valueHelpRequest="onRequestWBS"
											showValueHelp="true" />
									</VBox>
									<VBox justifyContent="Center">
										<Text text="{laborBonus>ActivityType}" class="customText"
											visible="{= !${laborBonus>Edit} }" />
										<Input
											editable="{= ${laborBonus>Edit} &amp;&amp; 
																	${laborBonus>IntStatus} !== '2' &amp;&amp; 
																	${laborBonus>IntStatus} !== '4' &amp;&amp; 
																	${laborBonus>IntStatus} !== '6' &amp;&amp; 
																	${laborBonus>IntStatus} !== '7'}"
											type="Text" value="{laborBonus>ActivityType}"
											class="customText" visible="{= ${laborBonus>Edit} }"
											width="100%" valueHelpRequest="onRequestActivityStdN"
											showValueHelp="true" />
									</VBox>
								</cells>
							</ColumnListItem>
						</items>
					</Table>
				</ScrollContainer>
			</content>
		</Panel>
		<List id="previewList" items="{preview>/}">
			<CustomListItem>
				<Panel class="customSubHeaderBackground foremanPrint"
					width="100%">
					<content>
						<Toolbar height="150px">
							<VBox>
								<VBox class="sapUiSmallMargin">
									<Label text="{preview>ProjectName}"
										labelFor="projectNamePreview"
										class="projectNameLabel myMasterLargeFont cutomTextColorPreview">
									</Label>
									<Text text="{preview>ProjectNo}"
										class="projectNameText myMasterLargeFont cutomTextColorPreview">
									</Text>
								</VBox>
								<VBox class="sapUiSmallMargin">
									<Label text="{preview>ForemanName}"
										labelFor="foremanNamePreview"
										class="foremanNameLabel myMasterMediumFont cutomTextColorPreview">
									</Label>
									<Text text="{preview>ForemanNo}"
										class="foremanNameText myMasterMediumFont cutomTextColorPreview">
									</Text>
								</VBox>
							</VBox>
							<ToolbarSpacer />
							<VBox class="sapUiSmallMargin">
								<Text text="{preview>TotalCost} {preview>Currency}"
									class="myMasterXLargeFont cutomTextColorPreview" />
								<Text text="{i18n>totalCost}"
									class="myMasterSmallFont cutomTextColorPreview sapUiLargeMarginBegin customTotalCost" />
								<Text
									text="{i18n>pointed} { path: 'preview>Day', formatter: '.formatter.formatDisplayDate' }"
									class="sapUiLargeMarginTop myMasterSmallFont cutomTextColorPreview"></Text>
							</VBox>
						</Toolbar>
					</content>
				</Panel>
				<Panel class="sapUiSmallMargin" width="98%">
					<content>
						<Toolbar height="45px"
							visible="{= ${preview>PrintingToLabor/results}.length !== 0}">
							<Text text="{i18n>Lab}"
								class="sapUiSmallMarginBottom customPreviewFont">
							</Text>
							<ToolbarSpacer />
							<VBox height="45px">
								<Button visible="{= !${preview>PrintingToLabor/Edit} }"
									icon="sap-icon://edit" press="onPressPreviewLaborEdit"
									type="Transparent" class="sapUiSmallMarginBottom printButton" />
								<HBox>
									<Button visible="{= ${preview>PrintingToLabor/Edit} }"
										icon="sap-icon://save" press="onPressPreviewLaborSave"
										type="Transparent" class="customSaveBtn" />
									<Button visible="{= ${preview>PrintingToLabor/Edit} }"
										icon="sap-icon://decline" press="onPressPreviewLaborCancel"
										type="Transparent" class="customSaveBtn" />
								</HBox>
							</VBox>
						</Toolbar>
						<ScrollContainer class="" height=""
							vertical="true">
							<Table items="{preview>PrintingToLabor/results/}"
								height="200px"
								class="sapUiNoMargin sapUiNoContentPadding customTable"
								visible="{= ${preview>PrintingToLabor/results}.length !== 0}">
								<columns class="sapUiNoContentPadding">
									<Column width="5rem">
										<Text text="{i18n>empNo}" />
									</Column>
									<Column width="8rem" hAlign="Left"
										minScreenWidth="768px" demandPopin="true">
										<Text text="{i18n>empName}" />
									</Column>
									<Column width="5rem" hAlign="Left"
										minScreenWidth="768px" demandPopin="true">
										<Text text="" />
									</Column>
									<Column width="8rem" hAlign="Left"
										minScreenWidth="768px" demandPopin="true">
										<Text text="" />
									</Column>
									<Column width="4rem" hAlign="Left"
										minScreenWidth="768px" demandPopin="true">
										<Text text="{i18n>costObj}" />
									</Column>
									<Column width="5rem" hAlign="Left"
										minScreenWidth="768px" demandPopin="true">
										<Text text="{i18n>Qty}" />
									</Column>
									<Column width="4rem" hAlign="Left"
										minScreenWidth="768px" demandPopin="true">
										<Text text="" />
									</Column>
								</columns>
								<items>
									<ColumnListItem
										highlight="{ 
												path: 'preview>IntStatus',
												formatter: '.formatter.setDetStatus'}">
										<cells>
											<Text text="{preview>EmployeeNo}" class="customText"></Text>
											<Text text="{preview>EmployeeName}" class="customText"></Text>
											<Text text=""></Text>
											<Text text=""></Text>
											<VBox justifyContent="Center">
												<Text text="{preview>CostCode}" class="customText"
													visible="{= !${preview>Edit} }" />
												<Input
													editable="{= ${preview>Edit} &amp;&amp; 
																		${preview>IntStatus} !== '2' &amp;&amp; 
																		${preview>IntStatus} !== '4' &amp;&amp; 
																		${preview>IntStatus} !== '6' &amp;&amp; 
																		${preview>IntStatus} !== '7'}"
													type="Text" value="{preview>CostCode}" class="customText"
													visible="{= ${preview>Edit} }" width="100%"
													valueHelpRequest="onRequestCostCode" showValueHelp="true" />
											</VBox>
											<VBox justifyContent="Center">
												<Text text="{preview>QtyDay} {preview>Unit}"
													class="customText" visible="{= !${preview>Edit} }" />
												<Input
													editable="{= ${preview>Edit} &amp;&amp; 
																		${preview>IntStatus} !== '2' &amp;&amp; 
																		${preview>IntStatus} !== '4' &amp;&amp; 
																		${preview>IntStatus} !== '6' &amp;&amp; 
																		${preview>IntStatus} !== '7'}"
													type="Text" value="{preview>QtyDay}" class="customText"
													visible="{= ${preview>Edit} }" width="100%" />
											</VBox>
											<Text text="" class="customText"></Text>
										</cells>
									</ColumnListItem>
								</items>
							</Table>
						</ScrollContainer>
					</content>
					<content>
						<Toolbar height="50px"
							visible="{= ${preview>PrintingToTemporary/results}.length !== 0}">
							<Text text="{i18n>Temp}"
								class="sapUiSmallMarginTopBottom customPreviewFont">
							</Text>
							<ToolbarSpacer />
							<VBox height="45px">
								<Button
									visible="{= !${preview>PrintingToTemporary/Edit} }"
									icon="sap-icon://edit" press="onPressPreviewTempEdit"
									type="Transparent" class="sapUiSmallMarginBottom printButton" />
								<HBox>
									<Button
										visible="{= ${preview>PrintingToTemporary/Edit} }"
										icon="sap-icon://save" press="onPressPreviewTempSave"
										type="Transparent" class="customSaveBtn" />
									<Button
										visible="{= ${preview>PrintingToTemporary/Edit} }"
										icon="sap-icon://decline" press="onPressPreviewTempCancel"
										type="Transparent" class="customSaveBtn" />
								</HBox>
							</VBox>
						</Toolbar>
						<ScrollContainer class="" height=""
							vertical="true">
							<Table items="{preview>PrintingToTemporary/results/}"
								height="200px"
								class="sapUiNoMargin sapUiNoContentPadding customTable"
								visible="{= ${preview>PrintingToTemporary/results}.length !== 0}">
								<columns class="sapUiNoContentPadding">
									<Column width="5rem">
										<Text text="{i18n>materialNo}" />
									</Column>
									<Column width="8rem" hAlign="Left"
										minScreenWidth="768px" demandPopin="true">
										<Text text="{i18n>desc}" />
									</Column>
									<Column width="5rem" hAlign="Left"
										minScreenWidth="768px" demandPopin="true">
										<Text text="{i18n>supNo}" />
									</Column>
									<Column width="8rem" hAlign="Left"
										minScreenWidth="768px" demandPopin="true">
										<Text text="{i18n>supName}" />
									</Column>
									<Column width="4rem" hAlign="Left"
										minScreenWidth="768px" demandPopin="true">
										<Text text="{i18n>costObj}" />
									</Column>
									<Column width="5rem" hAlign="Left"
										minScreenWidth="768px" demandPopin="true">
										<Text text="{i18n>Qty}" />
									</Column>
									<Column width="4rem" hAlign="Left"
										minScreenWidth="768px" demandPopin="true">
										<Text text="" />
									</Column>
								</columns>
								<items>
									<ColumnListItem
										highlight="{ 
												path: 'preview>IntStatus',
												formatter: '.formatter.setDetStatus'}">
										<cells>
											<Text text="{preview>MaterialNo}" class="customText"></Text>
											<Text text="{preview>MaterialDesc}" class="customText"></Text>
											<Text text="{preview>SupplierNo}" class="customText"></Text>
											<Text text="{preview>SupplierName}" class="customText"></Text>
											<VBox justifyContent="Center">
												<Text text="{preview>CostCode}" class="customText"
													visible="{= !${preview>Edit} }" />
												<Input
													editable="{= ${preview>Edit} &amp;&amp; 
																		${preview>IntStatus} !== '2' &amp;&amp; 
																		${preview>IntStatus} !== '4' &amp;&amp; 
																		${preview>IntStatus} !== '6' &amp;&amp; 
																		${preview>IntStatus} !== '7'}"
													type="Text" value="{preview>CostCode}" class="customText"
													visible="{= ${preview>Edit} }" width="100%"
													valueHelpRequest="onRequestCostCode" showValueHelp="true" />
											</VBox>
											<VBox justifyContent="Center">
												<Text text="{preview>QtyOperated} {preview>Unit}"
													class="customText" visible="{= !${preview>Edit} }" />
												<Input
													editable="{= ${preview>Edit} &amp;&amp; 
																		${preview>IntStatus} !== '2' &amp;&amp; 
																		${preview>IntStatus} !== '4' &amp;&amp; 
																		${preview>IntStatus} !== '6' &amp;&amp; 
																		${preview>IntStatus} !== '7'}"
													type="Text" value="{preview>QtyOperated}"
													class="customText" visible="{= ${preview>Edit} }"
													width="100%" />
											</VBox>
										</cells>
									</ColumnListItem>
								</items>
							</Table>
						</ScrollContainer>
					</content>
					<content>
						<Toolbar height="50px"
							visible="{= ${preview>PrintingToEquipment/results}.length !== 0}">
							<Text text="{i18n>Equip}"
								class="sapUiSmallMarginTopBottom customPreviewFont">
							</Text>
							<ToolbarSpacer />
							<VBox height="45px">
								<Button
									visible="{= !${preview>PrintingToEquipment/Edit} }"
									icon="sap-icon://edit" press="onPressPreviewEquipEdit"
									type="Transparent" class="sapUiSmallMarginBottom printButton" />
								<HBox>
									<Button
										visible="{= ${preview>PrintingToEquipment/Edit} }"
										icon="sap-icon://save" press="onPressPreviewEquipSave"
										type="Transparent" class="customSaveBtn" />
									<Button
										visible="{= ${preview>PrintingToEquipment/Edit} }"
										icon="sap-icon://decline" press="onPressPreviewEquipCancel"
										type="Transparent" class="customSaveBtn" />
								</HBox>
							</VBox>
						</Toolbar>
						<ScrollContainer class="" height=""
							vertical="true">
							<Table items="{preview>PrintingToEquipment/results/}"
								height="200px"
								class="sapUiNoMargin sapUiNoContentPadding customTable"
								visible="{= ${preview>PrintingToEquipment/results}.length !== 0}">
								<columns class="sapUiNoContentPadding">
									<Column width="5rem">
										<Text text="{i18n>materialNo}" />
									</Column>
									<Column width="8rem" hAlign="Left"
										minScreenWidth="768px" demandPopin="true">
										<Text text="{i18n>desc}" />
									</Column>
									<Column width="5rem" hAlign="Left"
										minScreenWidth="768px" demandPopin="true">
										<Text text="" />
									</Column>
									<Column width="8rem" hAlign="Left"
										minScreenWidth="768px" demandPopin="true">
										<Text text="" />
									</Column>
									<Column width="4rem" hAlign="Left"
										minScreenWidth="768px" demandPopin="true">
										<Text text="{i18n>costObj}" />
									</Column>
									<Column width="5rem" hAlign="Left"
										minScreenWidth="768px" demandPopin="true">
										<Text text="{i18n>Qty}" />
									</Column>
									<Column width="4rem" hAlign="Left"
										minScreenWidth="768px" demandPopin="true">
										<Text text="{i18n>status}" />
									</Column>
								</columns>
								<items>
									<ColumnListItem
										highlight="{ 
												path: 'preview>IntStatus',
												formatter: '.formatter.setDetStatus'}">
										<cells>
											<Text text="{preview>EquimentNo}" class="customText"></Text>
											<Text text="{preview>EquipmentName}" class="customText"></Text>
											<Text text=""></Text>
											<Text text=""></Text>
											<VBox justifyContent="Center">
												<Text text="{preview>CostCode}" class="customText"
													visible="{= !${preview>Edit} }" />
												<Input
													editable="{= ${preview>Edit} &amp;&amp; 
																		${preview>IntStatus} !== '2' &amp;&amp; 
																		${preview>IntStatus} !== '4' &amp;&amp; 
																		${preview>IntStatus} !== '6' &amp;&amp; 
																		${preview>IntStatus} !== '7'}"
													type="Text" value="{preview>CostCode}" class="customText"
													visible="{= ${preview>Edit} }" width="100%"
													valueHelpRequest="onRequestCostCode" showValueHelp="true" />
											</VBox>
											<VBox justifyContent="Center">
												<Text text="{preview>Quantity} {preview>Unit}"
													class="customText" visible="{= !${preview>Edit} }" />
